{
  "manifest_version": 3,
  "name": "Enhanced Voice Navigator",
  "version": "2.0",
  "description": "Advanced voice control for any webpage - navigate, fill forms, select dropdowns, and more",
  "permissions": ["activeTab", "scripting", "storage", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Enhanced Voice Navigator"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  }
}

